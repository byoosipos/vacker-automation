                    <div class="metric-value risk-score-${overall_risk_score > 70 ? 'high' : overall_risk_score > 40 ? 'medium' : 'low'}">
                        ${overall_risk_score}/100
                    </div>
                    <div class="metric-label">${this.getRiskLevelText(overall_risk_score)}</div>
                </div>
