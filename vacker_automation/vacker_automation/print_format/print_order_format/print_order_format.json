 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html>\n<head>\n    <meta charset=\"utf-8\">\n    <title>{{ doc.name }}</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n            font-size: 12px;\n            line-height: 1.4;\n        }\n        .header {\n            text-align: center;\n            font-size: 18px;\n            font-weight: bold;\n            margin-bottom: 30px;\n            border-bottom: 2px solid #000;\n            padding-bottom: 10px;\n        }\n        .section {\n            margin-bottom: 20px;\n        }\n        .section-title {\n            font-weight: bold;\n            font-size: 14px;\n            margin-bottom: 10px;\n            border-bottom: 1px solid #000;\n            padding-bottom: 5px;\n        }\n        .row {\n            display: flex;\n            margin-bottom: 8px;\n        }\n        .col-6 {\n            width: 50%;\n            padding-right: 10px;\n        }\n        .col-4 {\n            width: 33.33%;\n            padding-right: 10px;\n        }\n        .col-3 {\n            width: 25%;\n            padding-right: 10px;\n        }\n        .label {\n            font-weight: bold;\n            min-width: 120px;\n        }\n        .value {\n            flex: 1;\n        }\n        table {\n            width: 100%;\n            border-collapse: collapse;\n            margin: 10px 0;\n        }\n        th, td {\n            border: 1px solid #000;\n            padding: 8px;\n            text-align: left;\n            font-size: 11px;\n        }\n        th {\n            background-color: #f0f0f0;\n            font-weight: bold;\n        }\n        .approval-section {\n            margin-top: 30px;\n        }\n        .approval-row {\n            display: flex;\n            justify-content: space-between;\n            margin-bottom: 20px;\n        }\n        .approval-item {\n            width: 45%;\n        }\n        .signature-line {\n            border-top: 1px solid #000;\n            margin-top: 30px;\n            padding-top: 5px;\n        }\n        .remarks-section {\n            margin-top: 20px;\n        }\n        .remarks-box {\n            border: 1px solid #000;\n            padding: 10px;\n            margin: 10px 0;\n            min-height: 60px;\n        }\n        .page-break {\n            page-break-before: always;\n        }\n        .total-row {\n            font-weight: bold;\n            background-color: #f0f0f0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"header\">\n        PRINT ORDER REQUEST FORM\n    </div>\n\n    <!-- Client and Project Details -->\n    <div class=\"section\">\n        <div class=\"row\">\n            <div class=\"col-6\">\n                <span class=\"label\">CLIENT NAME / PROJECT DETAILS:</span>\n                <span class=\"value\">{{ doc.client_name or '' }}</span>\n            </div>\n            <div class=\"col-6\">\n                <span class=\"label\">CUSTOMER:</span>\n                <span class=\"value\">{{ doc.customer or '' }}</span>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-6\">\n                <span class=\"label\">ACCOUNT HOLDER:</span>\n                <span class=\"value\">{{ doc.account_holder or '' }}</span>\n            </div>\n            <div class=\"col-6\">\n                <span class=\"label\">SALES ORDER:</span>\n                <span class=\"value\">{{ doc.sales_order or '' }}</span>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-6\">\n                <span class=\"label\">LPO No.:</span>\n                <span class=\"value\">{{ doc.lpo_no or '' }}</span>\n            </div>\n            <div class=\"col-6\">\n                <span class=\"label\">DATE:</span>\n                <span class=\"value\">{{ doc.date or '' }}</span>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-6\">\n                <span class=\"label\">PROJECT DETAILS:</span>\n                <span class=\"value\">{{ doc.project_details or '' }}</span>\n            </div>\n        </div>\n    </div>\n\n    <!-- Job Details Table -->\n    <div class=\"section\">\n        <div class=\"section-title\">JOB DETAILS</div>\n        <table>\n            <thead>\n                <tr>\n                    <th>JOB DETAILS</th>\n                    <th>PRINT SIZES</th>\n                    <th>ORIENTATION</th>\n                    <th>MATERIAL</th>\n                    <th>QTY</th>\n                    <th>SQMTS</th>\n                    <th>TOTAL SQMTS</th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for item in doc.job_details_table %}\n                <tr>\n                    <td>{{ item.job_details or '' }}</td>\n                    <td>{{ item.print_sizes or '' }}</td>\n                    <td>{{ item.orientation or '' }}</td>\n                    <td>{{ item.material or '' }}</td>\n                    <td>{{ item.qty or 0 }}</td>\n                    <td>{{ item.sqmts or 0 }}</td>\n                    <td>{{ item.total_sqmts or 0 }}</td>\n                </tr>\n                {% endfor %}\n                <tr class=\"total-row\">\n                    <td colspan=\"6\" style=\"text-align: right;\"><strong>TOTAL SQMTS:</strong></td>\n                    <td><strong>{{ doc.total_sqmts or 0 }}</strong></td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n\n    <!-- Attachments -->\n    {% if doc.attachments_table %}\n    <div class=\"section\">\n        <div class=\"section-title\">ATTACHMENTS</div>\n        <table>\n            <thead>\n                <tr>\n                    <th>ATTACHMENT NAME</th>\n                    <th>FILE</th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for attachment in doc.attachments_table %}\n                <tr>\n                    <td>{{ attachment.attachment_name or '' }}</td>\n                    <td>{{ attachment.attachment_file or '' }}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n    </div>\n    {% endif %}\n\n    <!-- Approval Sections -->\n    <div class=\"approval-section\">\n        <div class=\"section-title\">ACKNOWLEDGED/REVIEWED BY</div>\n        <div class=\"approval-row\">\n            <div class=\"approval-item\">\n                <div class=\"label\">PRODUCTION MANAGER:</div>\n                <div class=\"signature-line\">{{ doc.production_manager or '' }}</div>\n            </div>\n            <div class=\"approval-item\">\n                <div class=\"label\">FINANCE MANAGER:</div>\n                <div class=\"signature-line\">{{ doc.finance_manager or '' }}</div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"approval-section\">\n        <div class=\"section-title\">APPROVED BY</div>\n        <div class=\"approval-row\">\n            <div class=\"approval-item\">\n                <div class=\"label\">SALES & MARKETING:</div>\n                <div class=\"signature-line\">{{ doc.sales_marketing or '' }}</div>\n            </div>\n            <div class=\"approval-item\">\n                <div class=\"label\">MANAGING DIRECTOR:</div>\n                <div class=\"signature-line\">{{ doc.managing_director or '' }}</div>\n            </div>\n        </div>\n    </div>\n\n    <!-- Remarks Section -->\n    <div class=\"remarks-section\">\n        <div class=\"section-title\">REMARKS</div>\n        <div class=\"row\">\n            <div class=\"col-6\">\n                <div class=\"label\">To Production/Operator:</div>\n                <div class=\"remarks-box\">{{ doc.remarks_production or '' }}</div>\n            </div>\n            <div class=\"col-6\">\n                <div class=\"label\">Finance/GM Comments:</div>\n                <div class=\"remarks-box\">{{ doc.remarks_finance or '' }}</div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-6\">\n                <div class=\"label\">Delivery Details:</div>\n                <div class=\"remarks-box\">{{ doc.remarks_delivery or '' }}</div>\n            </div>\n        </div>\n    </div>\n\n    <!-- Footer -->\n    <div style=\"margin-top: 50px; text-align: center; font-size: 10px; color: #666;\">\n        <p>Print Order: {{ doc.name }} | Generated on: {{ frappe.utils.nowdate() }} | Page 1 of 1</p>\n    </div>\n</body>\n</html>",
